<Window x:Class="Page.AddPictyre"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Page"
        mc:Ignorable="d"
        Title="AddPictyre" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Name="CodeNum"/>
            
            <Button Click="BChangePicture_Click" x:Name="BChangePicture" Content="ChangePicture" Margin="5"/>
            <Button Click="BSave_Click" x:Name="BSave" Content="Save" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
